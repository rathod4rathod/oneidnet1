<script>
    $('.netpro').click(function () {
        var s_value = $(this).val();
        $('#netpro_save').css('display', 'block');
        $('#netpro_save').click(function () {
            $.ajax({
                type: "POST",
                url: "preferences/netpro_update_preferences",
                data: {value: s_value},
                success: function (data)
                {
                    if (data)
                    {
                        $('#netpro_save').css('display', 'none');
                    }
                }
            });
        });

    });
    $('.buzzin').click(function () {
        var s_value = $(this).val();
        $('#buzzin_save').css('display', 'block');
        $('#buzzin_save').click(function () {
            $.ajax({
                type: "POST",
                url: "preferences/buzzin_update_preferences",
                data: {value: s_value},
                success: function (data)
                {
                    if (data)
                    {
                        $('#buzzin_save').css('display', 'none');
                    }
                }
            });
        });

    });
    $('.uclick').click(function () {
        var s_value = $(this).val();
        $('#click_save').css('display', 'block');
        $('#click_save').click(function () {
            $.ajax({
                type: "POST",
                url: "preferences/click_update_preferences",
                data: {value: s_value},
                success: function (data)
                {
                    if (data)
                    {
                        $('#click_save').css('display', 'none');
                    }
                }
            });
        });

    });
    $('.tunnel').click(function () {
        var s_value = $(this).val();
        $('#tunnel_save').css('display', 'block');
        $('#tunnel_save').click(function () {
            $.ajax({
                type: "POST",
                url: "preferences/tunnel_update_preferences",
                data: {value: s_value},
                success: function (data)
                {
                    if (data)
                    {
                        $('#tunnel_save').css('display', 'none');
                    }
                }
            });
        });

    });
    $('#location_access').on("change", function () {
        if ($('#location_access').val() == 'PARTICULAR') {
            $('#countries_list').css('display', 'block');

        }
        else if ($('#location_access').val() == 'ANY') {
            $('#countries_list').css('display', 'none');

        }
    });
    $('#device_access').on("change", function () {
        if ($('#device_access').val() == 'PARTICULAR') {
            $('#device_list').css('display', 'block');

        }
        else if ($('#device_access').val() == 'ANY') {
            $('#device_list').css('display', 'none');

        }
    });
    $('#browser_access').on("change", function () {
        if ($('#browser_access').val() == 'PARTICULAR') {
            $('#browser_list').css('display', 'block');

        }
        else if ($('#browser_access').val() == 'ANY') {
            $('#browser_list').css('display', 'none');

        }
    });
    $('#os_access').on("change", function () {
        if ($('#os_access').val() == 'PARTICULAR') {
            $('#os_list').css('display', 'block');

        }
        else if ($('#os_access').val() == 'ANY') {
            $('#os_list').css('display', 'none');
        }
    });
    $('#symantics_save').click(function () {
        var location_access = $('#location_access').val();
        var device_access = $('#device_access').val();
        var browser_access = $('#browser_access').val();
        var os_access = $('#os_access').val();
        var countries = $('#multi_countries').val();
        var devices = $('#multi_devices').val();
        var browsers = $('#multi_browsers').val();
        var operatingsystems = $('#multi_operatingsystems').val();
        $.ajax({
            type: "POST",
            url: "preferences/save_symantics",
            data: {location_access: location_access, device_access: device_access, browser_access: browser_access, os_access: os_access,
                countries: countries, devices: devices, browsers: browsers, operatingsystems: operatingsystems},
            success: function (data) {
                window.location.reload();
            }
        });
    });
    $('#symantics_cancel').click(function () {
        $('.click_NotificationListWrapper').css('display', 'none');
    });
</script>
