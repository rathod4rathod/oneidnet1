var slogans=["World as One","One World Net","World Id net","One ID World",];
var i=1;

function change(){
	setTimeout(function() {
		document.getElementById("slogan").innerHTML = slogans[i];
		i++;
		if(i<(slogans.length)){
			change();
		}
		else{
			i=0;
			change();}
	}, 8000);
}	


    
    
    
